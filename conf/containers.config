process {
    withLabel: biokanga {
      container = 'rsuchecki/biokanga:4.3.9'
    }
    withLabel: bowtie2 {
      container = 'alperyilmaz/bowtie2-samtools' //'alexiswl/bowtie2_2.3.3.1'// 'genomicpariscentre/bowtie2:2.2.4'
    }
    withLabel: bwa {
      container = 'dukegcb/bwa-samtools:0.7.12_1.2.1-254-6462e34' //'genomicpariscentre/bwa:v0.7.15' //'alexcoppe/bwa'//'genomicpariscentre/bwa:v0.7.15'
    }
    withLabel: dwgsim {
      container = 'genomicpariscentre/dwgsim:0.1.11'
    }
    // withLabel: fastx {
    //   container = //'alexcoppe/fastx-toolkit-lite'
    // }
    withLabel: rnftools {
      container = 'alperyilmaz/conda-rnftools'
    }
    withLabel: rscript {
      container = 'dceoy/r-tidyverse' //'rocker/tidyverse:3.4.0' //'genomicpariscentre/ggplot2:2.2.1'
    }
    withLabel: samtools {
      container = 'genomicpariscentre/samtools:1.4.1'
    }
}
